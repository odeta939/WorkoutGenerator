{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "exercises": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "howToPerform": {
            "type": "string"
          },
          "muscleGroup": {
            "type": "string"
          },
          "repetitions": {
            "type": "string"
          },
          "weightType": {
            "type": "string"
          },
          "kilograms": {
            "type": "string"
          }
        },
        "required": ["name", "howToPerform", "muscleGroup", "repetitions", "weightType", "kilograms"]
      }
    }
  },
  "required": ["exercises"]
}